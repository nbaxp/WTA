@section scripts{
    <template id="page">
        <div>{{count}}</div>
        <button @@click="count++">click</button>
    </template>
    <script type="module">
        import { ref } from 'vue';
        var url = '@this.Context.Request.GetDisplayUrl()';
        var options = { headers: { accept: 'application/json' } };
        fetch(url, options).then(o => o.json()).then(o => console.log(o));
        window.page = {
            template: '#page',
            setup() {
                const count = ref(0);
                return {
                    count
                }
            }
        };
    </script>
}
