{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "UseNacos": true,
  "NacosConfig": {
    "Listeners": [
      {
        "Optional": true,
        "DataId": "appsettings.json",
        "Group": "DEFAULT_GROUP"
      }
    ],
    "ServerAddresses": [ "http://nacos:8848" ]
  }

}
